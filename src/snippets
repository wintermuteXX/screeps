function sorter(structure)
{
	switch (structure.structureType)
	{
		case STRUCTURE_SPAWN:
			return 1;
		case STRUCTURE_EXTENSION:
			return 2;
		case STRUCTURE_TOWER:
			return 3;
		default:
			return 4;
	}
}
